<class name="Editor">
    <relationships>
        <association name="parent" class="Window" min="1" max="1" />
    </relationships>
    <method name="Editor">
        <parameter name="attr_name" />
        <parameter name="attr_type" />
        <body>
            self.attr_name = attr_name
            self.attr_type = attr_type
        </body>
    </method>
    <scxml initial="idle">
        <state id="idle" />
    </scxml>
</class>